<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>RenderBuddy</id>
		<version>2.0.9</version>
		<title>RenderBuddy</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/RenderBuddy</projectUrl>
		<licenseUrl>https://github.com/dmanning23/RenderBuddy/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MonoGame library to simplify drawing normal-mapped images</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>android, ios, windows universal, monogame, xamarin</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.7"/>
				<dependency id="CameraBuddy" version="2.0"/>
				<dependency id="FilenameBuddy" version="2.0"/>
				<dependency id="GameTimer" version="2.0"/>
				<dependency id="MatrixExtensions" version="2.0"/>
				<dependency id="PrimitiveBuddy" version="2.0"/>
				<dependency id="RandomExtensions.dmanning23" version="2.0"/>
				<dependency id="ResolutionBuddy" version="2.0"/>
				<dependency id="Vector2Extensions" version="2.0"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.7"/>
				<dependency id="CameraBuddy" version="2.0"/>
				<dependency id="FilenameBuddy" version="2.0"/>
				<dependency id="GameTimer" version="2.0"/>
				<dependency id="MatrixExtensions" version="2.0"/>
				<dependency id="PrimitiveBuddy" version="2.0"/>
				<dependency id="RandomExtensions.dmanning23" version="2.0"/>
				<dependency id="ResolutionBuddy" version="2.0"/>
				<dependency id="Vector2Extensions" version="2.0"/>
			</group>


			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.WindowsDX" version="3.7"/>
				<dependency id="CameraBuddy" version="2.0"/>
				<dependency id="FilenameBuddy" version="2.0"/>
				<dependency id="GameTimer" version="2.0"/>
				<dependency id="MatrixExtensions" version="2.0"/>
				<dependency id="PrimitiveBuddy" version="2.0"/>
				<dependency id="RandomExtensions.dmanning23" version="2.0"/>
				<dependency id="ResolutionBuddy" version="2.0"/>
				<dependency id="Vector2Extensions" version="2.0"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<file src="..\Content\AnimationBuddyShader.fx" target="content\Content\AnimationBuddyShader.fx" />

		<!--Xamarin.Android-->
		<file src="RenderBuddy.Android\bin\Release\RenderBuddy.Android.pdb" target="lib\MonoAndroid10\RenderBuddy.Android.pdb" />
		<file src="RenderBuddy.Android\bin\Release\RenderBuddy.Android.dll" target="lib\MonoAndroid10\RenderBuddy.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="RenderBuddy.iOS\bin\Release\RenderBuddy.iOS.pdb" target="lib\Xamarin.iOS10\RenderBuddy.iOS.pdb" />
		<file src="RenderBuddy.iOS\bin\Release\RenderBuddy.iOS.dll" target="lib\Xamarin.iOS10\RenderBuddy.iOS.dll" />
		
		<!--WindowsDX-->
		<file src="RenderBuddy.WindowsDX\bin\Release\RenderBuddy.WindowsDX.pdb" target="lib\net451\RenderBuddy.WindowsDX.pdb" />
		<file src="RenderBuddy.WindowsDX\bin\Release\RenderBuddy.WindowsDX.dll" target="lib\net451\RenderBuddy.WindowsDX.dll" />

	</files>
</package>
