<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>RenderBuddy</id>
		<version>1.0.17</version>
		<title>RenderBuddy</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/RenderBuddy</projectUrl>
		<licenseUrl>https://github.com/dmanning23/RenderBuddy/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MonoGame library to simplify drawing normal-mapped images</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.5"/>
				<dependency id="CameraBuddy" version="1.0"/>
				<dependency id="FilenameBuddy" version="1.0.2"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="ResolutionBuddy" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.5"/>
				<dependency id="CameraBuddy" version="1.0"/>
				<dependency id="FilenameBuddy" version="1.0.2"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="ResolutionBuddy" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

			<!--Win Universal-->
			<group targetFramework="uap10.0">
				<dependency id="MonoGame.Framework.WindowsUniversal" version="3.5"/>
				<dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.1"/>
				<dependency id="CameraBuddy" version="1.0"/>
				<dependency id="FilenameBuddy" version="1.0.2"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="ResolutionBuddy" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.DesktopGL" version="3.5"/>
				<dependency id="CameraBuddy" version="1.0"/>
				<dependency id="FilenameBuddy" version="1.0.2"/>
				<dependency id="GameTimer" version="1.0"/>
				<dependency id="MatrixExtensions" version="1.0.1"/>
				<dependency id="PrimitiveBuddy" version="1.0"/>
				<dependency id="RandomExtensions.dmanning23" version="1.0"/>
				<dependency id="ResolutionBuddy" version="1.0"/>
				<dependency id="Vector2Extensions" version="1.0.1"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<file src="..\Content\AnimationBuddyShader.fx" target="content\Content\AnimationBuddyShader.fx" />

		<!--Xamarin.Android-->
		<file src="RenderBuddy.Android\bin\Release\RenderBuddy.Android.pdb" target="lib\MonoAndroid10\RenderBuddy.Android.pdb" />
		<file src="RenderBuddy.Android\bin\Release\RenderBuddy.Android.dll" target="lib\MonoAndroid10\RenderBuddy.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="RenderBuddy.iOS\bin\Release\RenderBuddy.iOS.pdb" target="lib\Xamarin.iOS10\RenderBuddy.iOS.pdb" />
		<file src="RenderBuddy.iOS\bin\Release\RenderBuddy.iOS.dll" target="lib\Xamarin.iOS10\RenderBuddy.iOS.dll" />

		<!--Windows Universal-->
		<file src="RenderBuddy.WindowsUniversal\bin\Release\RenderBuddy.WindowsUniversal.pdb" target="lib\uap10.0\RenderBuddy.WindowsUniversal.pdb" />
		<file src="RenderBuddy.WindowsUniversal\bin\Release\RenderBuddy.WindowsUniversal.dll" target="lib\uap10.0\RenderBuddy.WindowsUniversal.dll" />

		<!--DesktopGL-->
		<file src="RenderBuddy.DesktopGL\bin\Release\RenderBuddy.DesktopGL.pdb" target="lib\net451\RenderBuddy.DesktopGL.pdb" />
		<file src="RenderBuddy.DesktopGL\bin\Release\RenderBuddy.DesktopGL.dll" target="lib\net451\RenderBuddy.DesktopGL.dll" />

	</files>
</package>
